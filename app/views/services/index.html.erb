<h1>Listar Servicios</h1>

<table>
  <tr>
    <th>Tipo</th>
    <th>Descripcion</th>
    <th>Tarifa</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @services.each do |service| %>
  <tr>
    <td><%= service.tipo %></td>
    <td><%= service.descripcion %></td>
    <td><%= service.tarifa %></td>
    <td><%= link_to 'Mostrar', service %></td>
    <td><%= link_to 'Editar', edit_service_path(service) %></td>
    <td><%= link_to 'Eliminar', service, confirm: 'Esta Seguro?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'Nuevo Servicio', new_service_path %>
